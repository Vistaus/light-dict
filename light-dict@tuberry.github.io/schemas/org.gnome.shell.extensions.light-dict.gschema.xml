<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.light-dict" path="/org/gnome/shell/extensions/light-dict/">
    <key type="b" name="sensitive-mode">
        <default>true</default>
        <summary>sensitive mode</summary>
    </key>
    <key type="b" name="hide-panel-title">
        <default>true</default>
        <summary>hide panel title</summary>
    </key>
    <key type="b" name="command-editable">
        <default>true</default>
        <summary>enable command edit</summary>
    </key>
    <key type="b" name="enable-shortcut">
        <default>true</default>
        <summary>enable shortcut</summary>
    </key>
    <key type="b" name="enable-strip">
        <default>false</default>
        <summary>enable text strip</summary>
    </key>
    <key type="as" name="short-cut">
        <default><![CDATA[['<Control><Shift>z']]]></default>
        <summary>short-cuts to trigger popup</summary>
    </key>
    <key type="as" name="autopopup-trigger">
        <default><![CDATA[['<Control><Alt>a']]]></default>
        <summary>short-cuts to trigger iconbar</summary>
    </key>
    <key type="u" name="autohide-timeout">
        <default>2500</default>
        <summary>autohide popup interval</summary>
    </key>
    <key type="u" name="icon-pagesize">
        <default>0</default>
        <summary>icon bar page size</summary>
    </key>
    <key type="u" name="trigger-style">
        <default>0</default>
        <summary>trigger style of popup panel</summary>
    </key>
    <key type="u" name="log-level">
        <default>0</default>
        <summary>log level in auto mode</summary>
    </key>
    <key type="i" name="x-offset">
        <default>0</default>
        <summary>x offset of icon bar</summary>
    </key>
    <key type="i" name="y-offset">
        <default>0</default>
        <summary>y offset of icon bar</summary>
    </key>
    <key type="s" name="selection-filter">
        <default>'^[^\\n\\.\\t\\/:]{3,50}$'</default>
        <summary>selection filter in auto mode</summary>
    </key>
    <key type="s" name="open-url">
        <default>"https://www.google.com/search?q=LDWORD"</default>
        <summary>open-url when right click on panel</summary>
    </key>
    <key type="s" name="white-list">
        <default>"*"</default>
        <summary>application white list</summary>
    </key>
    <key type="s" name="dict-command">
        <default>"dict -- LDWORD"</default>
        <summary>command in auto mode</summary>
    </key>
    <key type="s" name="click-command">
        <default>""</default>
        <summary>command when clicking popup panel</summary>
    </key>
    <key type="as" name="icon-commands">
        <default>[]</default>
        <summary>icon bar commands</summary>
    </key>
    <key type="as" name="icon-commands-active">
        <default>[]</default>
        <summary>active icon bar commands</summary>
    </key>
  </schema>
</schemalist>
