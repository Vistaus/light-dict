<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.light-dict" path="/org/gnome/shell/extensions/light-dict/">
    <key type="b" name="default-theme">
        <default>true</default>
        <summary>use default theme</summary>
    </key>
    <key type="b" name="hide-panel-title">
        <default>true</default>
        <summary>hide panel title</summary>
    </key>
    <key type="b" name="enable-strip">
        <default>false</default>
        <summary>enable text strip</summary>
    </key>
    <key type="b" name="enable-tooltips">
        <default>false</default>
        <summary>enable tooltips for icon bar</summary>
    </key>
    <key type="b" name="enable-systray">
        <default>true</default>
        <summary>enable systray</summary>
    </key>
    <key type="u" name="passive-mode">
        <range min="0" max="1"/>
        <default>0</default>
        <summary>need modifier to trriger(1) or not(0)</summary>
    </key>
    <key type="u" name="autohide-timeout">
        <default>2500</default>
        <summary>autohide popup interval</summary>
    </key>
    <key type="u" name="icon-pagesize">
        <default>0</default>
        <summary>icon bar page size</summary>
    </key>
    <key type="u" name="trigger-style">
        <default>0</default>
        <summary>trigger style of popup</summary>
    </key>
    <key type="u" name="app-list-type">
        <range min='0' max="1"/>
        <default>0</default>
        <summary>app list type: 0-block 1-allow</summary>
    </key>
    <key type="u" name="log-level">
        <default>0</default>
        <summary>log level in auto mode</summary>
    </key>
    <key type="i" name="x-offset">
        <default>0</default>
        <summary>x offset of icon bar</summary>
    </key>
    <key type="s" name="selection-filter">
        <default>'^[^\\n\\.\\t\\/:]{3,50}$'</default>
        <summary>selection filter in auto mode</summary>
    </key>
    <key type="s" name="right-click-command">
        <default>""</default>
        <summary>command when right-clicking popup box</summary>
    </key>
    <key type="s" name="application-list">
        <default>"*"</default>
        <summary>application black/white list</summary>
    </key>
    <key type="s" name="dict-command">
        <default>"dict -- LDWORD"</default>
        <summary>command in auto mode</summary>
    </key>
    <key type="s" name="left-click-command">
        <default>""</default>
        <summary>command when left-clicking popup box</summary>
    </key>
    <key type="as" name="iconbar-commands">
        <default>[]</default>
        <summary>icon bar commands</summary>
    </key>
  </schema>
</schemalist>
